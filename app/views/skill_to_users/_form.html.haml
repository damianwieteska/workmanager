= form_for @skill_to_user do |f|
  - if @skill_to_user.errors.any?
    #error_explanation
      %h2= "#{pluralize(@skill_to_user.errors.count, "error")} prohibited this skill_to_user from being saved:"
      %ul
        - @skill_to_user.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :level
    = f.number_field :level
  .field
    = f.label :user
    = f.text_field :user
  .field
    = f.label :skill
    = f.text_field :skill
  .actions
    = f.submit 'Save'
